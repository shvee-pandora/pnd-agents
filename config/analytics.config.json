  },
  "agents": [
    "Task Manager Agent",
    "Figma Reader Agent",
    "Frontend Engineer Agent",
    "Code Review Agent",
    "Unit Test Agent",
    "Sonar Validation Agent",
    "QA Agent",
    "Performance Agent",
    "Amplience CMS Agent",
    "Commerce Agent",
    "Broken Experience Detector Agent"
